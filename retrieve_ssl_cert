#Save to file
echo -n | openssl s_client -servername www.oceandns.net -connect www.oceandns.net:443 | sed -ne '/-BEGIN CERTIFICATE-/,/-END CERTIFICATE-/p' > cert.pem


#Expiration date
echo | openssl s_client -connect oceandns.nl:443 -servername oceandns.nl 2>/dev/null | openssl x509 -noout -enddate | sed 's/notAfter=//'
