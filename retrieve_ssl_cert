#Save to file
echo | openssl s_client -connect oceandns.net:443 -servername oceandns.net 2>/dev/null | sed -ne '/-BEGIN CERTIFICATE-/,/-END CERTIFICATE-/p' > cert.pem

#Expiration date
echo | openssl s_client -connect oceandns.net:443 -servername oceandns.net 2>/dev/null | openssl x509 -noout -enddate | sed 's/notAfter=//'

#Expiration date to unixtime
echo | openssl s_client -connect oceandns.nl:443 -servername oceandns.nl 2>/dev/null | openssl x509 -noout -enddate | sed 's/notAfter=//'| xargs -i date -d "{}" +%s

TLSA Record
echo | openssl s_client -connect oceandns.nl:443 -servername oceandns.nl 2>/dev/null | openssl x509 -noout -pubkey | openssl pkey -pubin -outform DER | openssl dgst -sha256 -binary | hexdump -ve '/1 "%02x"'
