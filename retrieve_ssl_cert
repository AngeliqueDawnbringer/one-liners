#Save to file
echo | openssl s_client -servername oceandns.net -connect oceandns.net:443 2>/dev/null | sed -ne '/-BEGIN CERTIFICATE-/,/-END CERTIFICATE-/p' > cert.pem


#Expiration date
echo | openssl s_client -connect oceandns.net:443 -servername oceandns.net 2>/dev/null | openssl x509 -noout -enddate | sed 's/notAfter=//'
